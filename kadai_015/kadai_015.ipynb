      "source": [
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"{self.name}さんは{self.age}歳です。\")"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "taro = Human(\"侍太郎\", 36)\n",
        "taro.printinfo()"
      ],
